{
    "compilerOptions": {
        "strict": true,
        "jsx": "react-jsx",
        "jsxImportSource": "hono/jsx",
        "noEmit": true,
        "noUnusedParameters": true,
        "noUnusedLocals": true,
        "skipLibCheck": true
    },
    "rootDir": "src",
    "include": ["src"],
    "exclude": ["node_modules", "dist"]
}
